{
  "name": "multipotjs",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "aliases": {
    "@": "build"
  },
  "scripts": {
    "start": "npx tsx src/Main.ts",
    "dev": "npx tsx watch src/Main.ts",
    "production": "cross-env NODE_ENV=production node --loader esm-module-alias/loader --no-warnings build/Main.js",
    "build": "tsc",
    "db:generate": "drizzle-kit generate:mysql --schema src/db/Schema.ts --out=src/db/migrations",
    "db:migrate": "npx tsx src/db/Migrate.ts",
    "db:seed": "npx tsx src/db/Seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.14.8",
    "nodemon": "^3.1.4",
    "tsx": "^4.15.7",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@types/geoip-lite": "^1.4.4",
    "@types/ssh2": "^1.15.0",
    "cross-env": "^7.0.3",
    "discord-webhook-node": "^1.1.8",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.20.14",
    "drizzle-orm": "^0.30.4",
    "esm-module-alias": "^2.2.0",
    "geoip-lite": "^1.4.10",
    "mysql2": "^3.10.1",
    "ssh2": "^1.15.0",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
